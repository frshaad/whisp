{
  "root": true,
  "extends": ["standard", "next/core-web-vitals", "prettier"],
  "plugins": ["simple-import-sort"],
  "globals": { "React": "writable" },
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "no-console": "error",
    "no-duplicate-imports": "error"
  }
}
